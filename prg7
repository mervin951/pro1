#include <stdio.h>
int main() 
{
    int a = 16;
    int b = 5;
    printf("\n a = %d b = %d", a, b);
    a += b;
    printf("\n a += b => %d", a);
    a -= b;
    printf("\n a -= b => %d", a);
    a *= b;
    printf("\n a *= b => %d", a);
    a /= b;
    printf("\n a /= b => %d", a);
    a %= b;
    printf("\n a %%= b => %d", a);
    return 0;
}
