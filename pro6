#include <stdio.h>
void main() 
{
    int a = 5, b = 6;
    printf("\na = %d b = %d", a, b);
    printf("\n a == b = %d", a == b);
    printf("\n a != b = %d", a != b);
    printf("\n a > b = %d", a > b);
    printf("\n a < b = %d", a < b);
    printf("\n a >= b = %d", a >= b); 
    printf("\n a <= b = %d", a <= b);
}
